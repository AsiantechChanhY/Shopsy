<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <RelativeLayout
        android:id="@+id/relativelay_shippingprofile"
        android:layout_width="fill_parent"
        android:layout_height="57dp"
        android:layout_alignParentTop="true"
        android:background="#149bb9" >

        <TextView
            android:id="@+id/shippingprofile_textViewnewshippingprofile"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:text="@string/newshipingprofile"
            android:textColor="#ffffff"
            android:textSize="17sp"
            android:textStyle="bold"
            android:typeface="serif" />

        <ImageView
            android:id="@+id/shippingprofile_addimage"
            android:layout_width="20dp"
            android:layout_height="30dp"
            android:layout_centerVertical="true"
            android:layout_marginRight="10dp"
            android:layout_toLeftOf="@+id/shippingprofile_textViewnewshippingprofile"
            android:contentDescription="@string/description"
            android:src="@drawable/shippling_profile_plus" />
    </RelativeLayout>

    <ScrollView
        android:id="@+id/myview1"
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:layout_below="@+id/relativelay_shippingprofile"
        android:background="#f1f1f1"
        android:orientation="vertical" >

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="wrap_content"
            android:background="#f1f1f1"
            android:orientation="vertical" >

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="fill_parent"
                android:layout_marginTop="1dip"
                android:background="#f1f1f1"
                android:orientation="vertical" >

                <RelativeLayout
                    android:id="@+id/linearlayout_shipping_layouts"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="15dp"
                    android:layout_marginRight="15dp"
                    android:layout_marginTop="10dp"
                    android:background="#ffffff"
                    android:orientation="vertical" >

                    <TextView
                        android:id="@+id/textView_shipping_profile_detail"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="10dp"
                        android:text="@string/shipingprodetil"
                        android:textSize="17sp"
                        android:textStyle="bold"
                        android:typeface="serif" />

                    <TextView
                        android:id="@+id/textView_shipping_profile_tittle1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:visibility="gone"
                        android:layout_below="@+id/textView_shipping_profile_detail"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="10dp"
                        android:text="Title :"
                        android:textSize="15sp"
                        android:typeface="serif" />

                    <TextView
                        android:id="@+id/textView_shipping_profile_tittle"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:visibility="gone"
                        android:layout_alignBaseline="@+id/textView_shipping_profile_tittle1"
                        android:layout_below="@+id/textView_shipping_profile_detail"
                        android:layout_toRightOf="@+id/textView_shipping_profile_tittle1"
                        android:textColor="#707070"
                        android:text=""
                        android:textSize="17sp"/>

                    <TextView
                        android:id="@+id/textView_shipping_profile_time1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textView_shipping_profile_tittle"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="10dp"
                        android:text="Time :"
                        android:textSize="15sp"
                        android:typeface="serif" />

                    <TextView
                        android:id="@+id/textView_shipping_profile_time"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_alignBaseline="@+id/textView_shipping_profile_time1"
                        android:layout_toRightOf="@+id/textView_shipping_profile_time1"
                        android:layout_below="@+id/textView_shipping_profile_tittle"
                        android:text=""
                        android:textColor="#707070"
                        android:textSize="17sp"/>

                    <TextView
                        android:id="@+id/textView_shipping_profile_from1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textView_shipping_profile_time"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="10dp"
                        android:text="Ship From :"
                        android:textSize="15sp"
                        android:typeface="serif" />

                    <TextView
                        android:id="@+id/textView_shipping_profile_from"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBaseline="@+id/textView_shipping_profile_from1"
                        android:layout_toRightOf="@+id/textView_shipping_profile_from1"
                        android:layout_below="@+id/textView_shipping_profile_time"
                        android:layout_marginRight="8dp"
                        android:layout_marginLeft="10dp"
                        android:text=""
                        android:textColor="#707070"
                        android:textSize="17sp" />

                    <TextView
                        android:id="@+id/textView_shipping_profile_desc1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textView_shipping_profile_from"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="10dp"
                        android:text="Ship To :"
                        android:textSize="15sp"
                        android:typeface="serif" />

                    <TextView
                        android:id="@+id/textView_shipping_profile_desc"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignBaseline="@+id/textView_shipping_profile_desc1"
                        android:layout_toRightOf="@+id/textView_shipping_profile_desc1"
                        android:layout_below="@+id/textView_shipping_profile_from"
                        android:layout_marginRight="8dp"
                        android:layout_marginLeft="10dp"
                        android:text=""
                        android:textColor="#707070"
                        android:textSize="17sp"/>

                    <TextView
                        android:id="@+id/textView_shipping_profile_cost1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textView_shipping_profile_desc"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="10dp"
                        android:text="Cost :"
                        android:textSize="15sp"
                        android:typeface="serif" />

                    <TextView
                        android:id="@+id/textView_shipping_profile_cost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_alignBaseline="@+id/textView_shipping_profile_cost1"
                        android:layout_toRightOf="@+id/textView_shipping_profile_cost1"
                        android:layout_below="@+id/textView_shipping_profile_desc"
                        android:layout_marginRight="8dp"
                        android:text=""
                        android:textColor="#31B404"
                        android:textSize="17sp"/>

                    <TextView
                        android:id="@+id/textView_shipping_profile_ano_cost1"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textView_shipping_profile_cost"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:text="With Other Cost :"
                        android:textSize="15sp"
                        android:typeface="serif" />

                    <TextView
                        android:id="@+id/textView_shipping_profile_ano_cost"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginLeft="10dp"
                        android:layout_marginBottom="10dp"
                        android:layout_marginRight="8dp"
                        android:layout_alignBaseline="@+id/textView_shipping_profile_ano_cost1"
                        android:layout_toRightOf="@+id/textView_shipping_profile_ano_cost1"
                        android:layout_below="@+id/textView_shipping_profile_cost"
                        android:text=""
                        android:textColor="#31B404"
                        android:textSize="17sp"/>

                    <TextView
                        android:id="@+id/textView_shipping_profile_ano_cost1ff"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/textView_shipping_profile_ano_cost1"
                        android:layout_marginLeft="15dp"
                        android:layout_marginTop="10dp"
                        android:layout_marginBottom="10dp"
                        android:text=""
                        android:textSize="15sp"
                        android:typeface="serif" />

                    <ImageView
                        android:id="@+id/imageView_shipping_profile_erase"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_alignParentRight="true"
                        android:layout_alignTop="@+id/textView_shipping_profile_detail"
                        android:layout_marginRight="5dp"
                        android:contentDescription="@string/destinations"
                        android:src="@drawable/erase" />

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/shippingprofile_createshipping"
                    android:layout_width="fill_parent"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_vertical"
                    android:orientation="vertical" >

                    <ImageView
                        android:id="@+id/imageView2"
                        android:layout_width="90dp"
                        android:layout_height="90dp"
                        android:layout_centerHorizontal="true"
                        android:contentDescription="@string/destinations"
                        android:src="@drawable/shippping_profile" />

                    <TextView
                        android:id="@+id/textView3_ddgjlg"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center_horizontal"
                        android:layout_marginLeft="15dp"
                        android:layout_marginRight="15dp"
                        android:layout_marginTop="10dp"
                        android:text="@string/doublequotes"
                        android:textColor="#ffffff"
                        android:textSize="17sp"
                        android:typeface="serif" />

                    <TextView
                        android:id="@+id/textView2"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/imageView2"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="10dp"
                        android:text="@string/createprofile"
                        android:textColor="#149bb9"
                        android:textSize="20sp"
                        android:textStyle="bold"
                        android:typeface="serif" />
                </RelativeLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>

</RelativeLayout>